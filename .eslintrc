{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "globals": {
    "__PATH_PREFIX__": true
  },
  "plugins": ["jsx-a11y"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "react-app",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}
